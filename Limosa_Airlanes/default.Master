    <link href="tasarim.css" rel="stylesheet" />

     <style type="text/css">
         .auto-style1 {
            float: left;
            width: 125px;
            height: 30px;
            margin-top: 5px;
            background-color: white;
            background-image: url('temalar/btnarka1.jpg');
            color:  #0097A7;
            line-height: 30px;
            text-align: center;
            margin-right: 20px;
            font-size: 18px;
        }
        .auto-style2 {
            float: left;
            width: 140px;
            height: 30px;
            margin-top: 5px;
            background-color: white;
            background-image: url('temalar/btnarka1.jpg');
            color: #0097A7;
            line-height: 30px;
            text-align: center;
            margin-right: 20px;
            font-size: 18px;
        }
    </style>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>
   <form id="form1" runat="server">
    <div class="kutu">
      <div class="slider">
          
                
                 
                     <center><img src="gorseller/limosa.jpg" alt="" /></center>  
                   
                
           
             
    </div>
      
        <div class="menu">
            
            <div style="width: 1000px; height: 40px; float: right">
              <a href="default.aspx"><div class="menubuton" style=" background-repeat: repeat">
