                <asp:DataList ID="datalist_duyuru" runat="server" Width="100%">
                        <ItemTemplate>
                            <table class="auto-style1" style="background-color: #EFEFEF">
                                <tr>
                                    <td>
                                        <asp:Image ID="Image1" runat="server" Height="30px" ImageUrl='<%# Eval("haberlerResim") %>' Width="30px" />
