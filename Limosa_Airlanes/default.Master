                                </tr>
                                <tr>
                                    <td colspan="2" style="padding-left: 5px">
                                        <asp:Label ID="Label2" runat="server" ForeColor="#606060" Text='<%# Eval("kampanyalarIcerik") %>'></asp:Label>
